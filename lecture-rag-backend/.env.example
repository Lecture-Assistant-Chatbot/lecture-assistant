# Server
LOG_LEVEL=info
CORS_ALLOW_ALL=true

# Replace the `GOOGLE_CLOUD_PROJECT_ID` and `GOOGLE_CLOUD_LOCATION` values
# with appropriate values for your project.
export GOOGLE_CLOUD_PROJECT=GOOGLE_CLOUD_PROJECT_ID
export GOOGLE_CLOUD_LOCATION=global
export GOOGLE_GENAI_USE_VERTEXAI=True

# Matching Engine (use the numeric ID or full resource name)
VERTEX_AI_INDEX_ENDPOINT=your-vector-search-endpoint
VERTEX_AI_DEPLOYED_INDEX=your-deployed-index-id

# Gemini
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-2.5-flash

# Timeouts
HTTP_TIMEOUT_SECONDS=60
